<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
    	android:title="Default fuel units"
    	android:key="fuel"
    	android:entries="@array/fuel_list"
    	android:entryValues="@array/fuel_list"
    	android:defaultValue="Gallons"></ListPreference>
    <ListPreference
    	android:title="Default distance units"
    	android:key="distance"
    	android:entries="@array/distance_list"
    	android:entryValues="@array/distance_list"
    	android:defaultValue="Miles"></ListPreference>
</PreferenceScreen>
